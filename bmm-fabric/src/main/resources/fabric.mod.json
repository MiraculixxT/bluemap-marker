{
  "schemaVersion": 1,
  "id": "bm-marker",
  "version": "1.1.3",

  "name": "BlueMap Marker Manager",
  "description": "A BlueMap addition plugin. Give your players the option to place markers on the map",
  "authors": ["Miraculixx"],
  "contact": {
    "homepage": "https://mutils.de/",
    "sources": "https://github.com/MiraculixxT/bluemap-marker"
  },

  "license": "MIT",
  "icon": "assets/bm-marker/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
